{
  "name": "pms-fe",
  "private": true,
  "scripts": {
    "dev:pms-admin": "turbo run dev --filter=pms-admin",
    "build:pms-admin": "turbo run build --filter=pms-admin",
    "lint:pms-admin": "turbo run lint --filter=pms-admin",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2",
    "@pms/tsconfig": "workspace:*",
    "@pms/eslint-config": "workspace:*"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "pms-admin": "workspace:*"
  }
}
